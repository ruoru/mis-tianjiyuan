{
  "name": "mis-tianjiyuan",
  "version": "0.0.0",
  "description": "TianJiYuan Property co., ltd management information service website.",
  "main": "main.js",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "webpack": "^3.11.0"
  },
  "scripts": {
    "start": "npm run build:dev && npm run dev",
    "build:dev": "npm run copyFiles && env=dev webpack --progress --profile --colors",
    "dev": "env=dev webpack-dev-server -p --inline --hot",
    "build:prod": "npm run copyFiles && env=prod webpack --progress --profile --colors",
    "prod": "env=prod webpack-dev-server -p --inline --hot",
    "copyFiles": "rm -rf dist && cp -r public dist",
    "lint": "standard",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruoru/mis-tianjiyuan.git"
  },
  "author": "ruoru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruoru/mis-tianjiyuan/issues"
  },
  "homepage": "https://github.com/ruoru/mis-tianjiyuan#readme"
}
